   "source": [
    "from reportlab.lib.pagesizes import A4\n",
    "from reportlab.pdfgen import canvas\n",
    "\n",
    "c = canvas.Canvas(\"blank-a4.pdf\", pagesize=A4)\n",
    "c.showPage()\n",
    "c.save()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "839b5dba-075e-4967-ab5b-c5c9a6198ae2",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "from PyPDF2 import PdfReader, PdfWriter\n",
    "\n",
    "base = PdfReader(\"blank-a4.pdf\")\n",
    "overlay = PdfReader(\"overlay.pdf\") # replace here\n",
    "\n",
    "page = base.pages[0]\n",
    "page.merge_page(overlay.pages[0])\n",
    "\n",
    "writer = PdfWriter()\n",
    "writer.add_page(page)\n",
    "\n",
    "with open(\"output.pdf\", \"wb\") as f:\n",
    "    writer.write(f)\n"
   ]
  },
